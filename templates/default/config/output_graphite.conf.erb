output {
    if "metric" in [tags] {
    graphite {
      host => "<%= @output_graphite_host %>"
      metrics => [ 
        "events.<%= @logstash_host %>.rate_1m", "%{events.rate_1m}",
        "events.<%= @logstash_host %>.rate_5m", "%{events.rate_5m}",
        "events.<%= @logstash_host %>.rate_15m", "%{events.rate_15m}",
        "events.<%= @logstash_host%>.count",   "%{events.count}" ]
    }
  }
}